class Circle {
	float x; 
	float y;
	float r;

boolean growing = true;

Circle(float x_, float y_);
x = 1;
y = 1;
r = 50; 
}



void grow() {
if (growing)
	r= r + 1;
}
}

boolean edges() {
	return( x + r > width || x - r < o || y + r > height ||);
}


void show() {
stroke(255)
strokeWeight(2);
noFill();
	ellipse(x, y, r*2, r*2);
}
var bubbles = [];


var bubble = {
	for (var i = 0; i < 10000000; i++) {
	bubbles [i]
	x:random (0, width),
	y:random (0,height)
	display:function() {
	stroke (255);
	nofill();
	ellipse(this.x, this.y, 24, 24)
	},
	move: function() {
	this.x = this.x + random (-2,2);
	this.y = this.y = random (-2,2);
	}
}

function setup(){
	createCanvas(windowWidth,windowHeight);
}

function draw() {
	background (50);
	for (var i = 0; i < bubbles.length; i++) {
	bubble.move[i];
	bubble.display();
}
}